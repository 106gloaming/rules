[Mitm]
hostname = youtubei.googleapis.com

[Script]
http-response ^https:\/\/youtubei\.googleapis\.com\/youtubei\/v1\/browse\? script-path=https://raw.githubusercontent.com/app2smile/rules/master/js/youtube.js, requires-body=true, binary-body-mode=true, timeout=10, tag=youtube-proto
